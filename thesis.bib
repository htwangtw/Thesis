isbn = {1467-9280},
issn = {1467-9280},
journal = {Psychological Science},
keywords = {Adolescent,Adult,Aged,Attention,Female,Humans,Male,Memory,Middle Aged,Short-Term,Task Performance and Analysis,Thinking},
month = {apr},
