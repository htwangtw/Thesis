journal = {Experimental brain research},
keywords = {Future thought,Goals,Hippocampus,Mind-wandering},
month = {jul},
pmid = {27443852},
title = {{How do we decide what to do? Resting-state connectivity patterns and components of self-generated thought linked to the development of more concrete personal goals.}},
url = {http://link.springer.com/10.1007/s00221-016-4729-y http://www.ncbi.nlm.nih.gov/pubmed/27443852},
