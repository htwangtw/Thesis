journal = {Journal of Neuroscience},
keywords = {CONNECTIVITY,FACES,FAMILIARITY,MODE NETWORK,NEURAL SYSTEMS,PERCEPTION,RECOGNITION,TASK,THOUGHT,WORKING-MEMORY,cognitive control,default mode network,fMRI,long-term memory,n-back,working memory},
