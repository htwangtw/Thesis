pmid = {24448763},
title = {{Controlling the stream of thought: Working memory capacity predicts adjustment of mind-wandering to situational demands}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24448763 http://link.springer.com/10.3758/s13423-013-0580-3},
volume = {21},
