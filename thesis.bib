file = {:/storage.its.york.ac.uk/pshome/hw1012/My Documents/Mendeley Desktop/2016/Miller et al/Miller et al. - 2016 - Multimodal population brain imaging in the UK Biobank prospective epidemiological study.pdf:pdf},
