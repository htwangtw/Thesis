file = {:/storage.its.york.ac.uk/pshome/hw1012/My Documents/Mendeley Desktop/2013/Bailey, West, Kuffel/Bailey, West, Kuffel - 2013 - What would my avatar do Gaming, pathology, and risky decision making.pdf:pdf},
isbn = {1664-1078},
issn = {16641078},
journal = {Frontiers in Psychology},
keywords = {Decision making,Impulsivity,Pathological video game use,Reward processing,Risk,Video games},
month = {sep},
