pages = {120--125},
pmid = {23261640},
title = {{Escaping the here and now: Evidence for a role of the default mode network in perceptually decoupled thought}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1053811912011895},
volume = {69},
year = {2013}
}
