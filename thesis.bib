pages = {2928--2947},
pmid = {25931002},
title = {{Neural correlates of personal goal processing during episodic future thinking and mind-wandering: An ALE meta-analysis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25931002 http://doi.wiley.com/10.1002/hbm.22818},
