isbn = {1095-9572},
issn = {10538119},
journal = {NeuroImage},
keywords = {Connectivity-based parcellation,Default mode network,Functional decoding,Parietal lobe,Posterior cingulate cortex,Retrosplenial cortex,Statistical learning},
month = {feb},
