pages = {0956797616634068--},
pmid = {26993740},
publisher = {SAGE Publications},
title = {{On the Necessity of Distinguishing Between Unintentional and Intentional Mind Wandering.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26993740 http://pss.sagepub.com/content/early/2016/03/18/0956797616634068.full?oauth-code=9KUHIueAOKqmXxcRN1muBBCW561OP6kX},
volume = {27},
